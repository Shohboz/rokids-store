<li class="logo">
	<%= link_to store_path, id: "logo-container", class: "brand-logo"  do %>
		<object id="front-page-logo" class="object-brand" type="image/svg+xml" data="/assets/logo.svg">На главную</object>
    <% end %>
</li>
<% @categories && @categories.each do |category| %>
  <li>
    <%= link_to category.title, store_path(:category => category.id), class: "truncate waves-effect waves-light" %>
  </li>
<% end %>


<% if session[:user_id] %>
	<li class="dashboard">
		<%= link_to "Dashboard", admin_path, class: "truncate waves-effect waves-light" %>
	</li>
	<li class="dashboard">
		<%= link_to "Logout", logout_path, method: :delete, class: "truncate waves-effect waves-light" %>
	</li>
<% end %>